<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  <servlet-name>FormsServlets</servlet-name>
  <servlet-class>com.spring.mvc.FormsJspServlets</servlet-class>
  
  
    </servlet>
  
  <servlet-mapping>
  <servlet-name>FormsServlets</servlet-name>
  <url-pattern>/register.do</url-pattern>
  
  </servlet-mapping>
  
  <servlet>
  <servlet-name>LoginForms</servlet-name>
  <servlet-class>com.spring.mvc.Loginforms</servlet-class>
    </servlet>
  
  <servlet-mapping>
  <servlet-name>LoginForms</servlet-name>
  <url-pattern>/login.do</url-pattern>
  
  </servlet-mapping>
  
  <servlet>
  <servlet-name>AddServlet</servlet-name>
  <servlet-class>com.spring.mvc.AddServlet</servlet-class>
  
  
    </servlet>
  
  <servlet-mapping>
  <servlet-name>AddServlet</servlet-name>
  <url-pattern>/add.do</url-pattern>
  
  </servlet-mapping>
  
   <servlet>
  <servlet-name>ViewAllServlet</servlet-name>
  <servlet-class>com.spring.mvc.ViewAllServlet</servlet-class>
  
  
    </servlet>
  
  <servlet-mapping>
  <servlet-name>ViewAllServlet</servlet-name>
  <url-pattern>/viewall.do</url-pattern>
  
  </servlet-mapping>
  
  
  
</web-app>
